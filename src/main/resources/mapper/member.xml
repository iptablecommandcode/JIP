<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ACCOUNT">
    <select id="ID" resultType="Map" parameterType="Map">
	    select * from ACCOUNT
		where ID = #{ID};
    </select>

</mapper>